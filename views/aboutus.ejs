    <script>
        // Counter animation for statistics
        function animateCounters() {
            const counters = document.querySelectorAll('.re-hero-stat-number[data-count]');
            
            counters.forEach(counter => {
                const target = parseInt(counter.getAttribute('data-count'));
                const duration = 2000;
                const stepTime = Math.max(Math.floor(duration / target), 50);
                let current = 0;
                
                const timer = setInterval(() => {
                    current += 1;
                    counter.textContent = current;
                    if (current >= target) {
                        clearInterval(timer);
                        counter.textContent = target;
                    }
                }, stepTime);
            });
        }

        // Scroll animations
        function handleScrollAnimations() {
            const elements = document.querySelectorAll('.re-scroll-animate');
            const windowHeight = window.innerHeight;
            
            elements.forEach(element => {
                const elementTop = element.getBoundingClientRect().top;
                const elementVisible = 150;
                
                if (elementTop < windowHeight - elementVisible) {
                    element.classList.add('re-in-view');
                }
            });
        }

        // Initialize all functionality
        document.addEventListener('DOMContentLoaded', function() {
            // Start counter animation after a delay
            setTimeout(animateCounters, 500);
            
            // Handle scroll animations
            window.addEventListener('scroll', handleScrollAnimations);
            handleScrollAnimations();
            
            // Enhanced hover effects for cards
            const cards = document.querySelectorAll('.re-value-card, .re-team-card, .re-tech-item');
            
            cards.forEach(card => {
                card.addEventListener('mouseenter', function() {
                    this.style.transform = 'translateY(-10px)';
                });
                
                card.addEventListener('mouseleave', function() {
                    this.style.transform = 'translateY(0)';
                });
            });
        });

        // Floating elements animation
        function createFloatingElements() {
            const hero = document.querySelector('.re-hero-section');
            if (!hero) return;
            
            for (let i = 0; i < 5; i++) {
                const element = document.createElement('div');
                element.style.cssText = `
                    position: absolute;
                    width: ${Math.random() * 100 + 50}px;
                    height: ${Math.random() * 100 + 50}px;
                    background: rgba(255, 255, 255, 0.1);
                    border-radius: 50%;
                    top: ${Math.random() * 100}%;
                    left: ${Math.random() * 100}%;
                    animation: re-pulse ${Math.random() * 3 + 2}s ease-in-out infinite;
                    pointer-events: none;
                    z-index: 1;
                `;
                hero.appendChild(element);
            }
        }

        // Initialize floating elements
        setTimeout(createFloatingElements, 1500);
    </script>
